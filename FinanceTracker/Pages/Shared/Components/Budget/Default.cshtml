@model FinanceTracker.Models.Budget

<h1>Budget</h1>
<table class="table table-bordered table-striped table-hover">
	<thead>
	<tr>
		<th>Id</th>
		<th>Cash In</th>
		<th>Cash Out</th>
		<th>Disposable Income</th>
	</tr>

	</thead>
	<tbody>
	<tr>
		<td>@Model.Id</td>
		<td>$@Model.CashIn</td>
		<td>$@Model.CashOut</td>
		<td>$@Model.DisposableIncome</td>
	</tr>
	</tbody>

</table>